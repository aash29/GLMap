(define
	(problem city_problem)
	(:domain city_domain)
	(:objects 
		agent0 - agent )
	(:init
		(at agent0 loc_1_1)
		)
	(:action move
		 (:parameters ?agent ?from ?to)
		 (:precondition (and
		 	       (at ?agent ?from)
			       (con ?from ?to)
			       )
	         )
		(:effect (and
			(at ?agent ?to)
			(not (at ?agent ?from))
			)
		)
	)

(:goal )
)
